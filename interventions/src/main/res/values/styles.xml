<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MainLayout" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#00FF00</item>
        <item name="android:background">#386bb8</item>
    </style>

    <style name="ListLayout" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#000000</item>
        <item name="android:background">#386bb8</item>
    </style>

    <style name="ListMainRow" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#efefef</item>
    </style>

    <style name="ListSubRow" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#efefef</item>
    </style>

    <style name="ListSubRowLabel" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListSubRowValue" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#efefef</item>
    </style>

    <style name="ListEmpty" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#efefef</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_margin">30dip</item>
    </style>

    <style name="FormLayout" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#00FF00</item>
        <item name="android:background">#386bb8</item>
    </style>

    <style name="FormLabel" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#dedede</item>
    </style>

    <style name="EditText" parent="@android:style/TextAppearance.Medium"></style>

    <style name="EditTextSmall" parent="@android:style/TextAppearance.Small"></style>

    <style name="EditNotes" parent="@android:style/TextAppearance.Medium">
        <item name="android:background">#efefef</item>
        <item name="android:textColor">#404040</item>
        <item name="android:layout_margin">5dip</item>
        <item name="android:bottomLeftRadius">7dip</item>
        <item name="android:bottomRightRadius">7dip</item>
        <item name="android:topLeftRadius">7dip</item>
        <item name="android:topRightRadius">7dip</item>
    </style>

    <style name="AdView">
        <item name="android:background">#000000</item>
    </style>

    <style name="CopyrightBanner" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#386bb8</item>
        <item name="android:background">#cdcdde</item>
    </style>

    <style name="Button">
        <item name="android:bottomLeftRadius">7dip</item>
        <item name="android:bottomRightRadius">7dip</item>
        <item name="android:topLeftRadius">7dip</item>
        <item name="android:topRightRadius">7dip</item>
    </style>

</resources>